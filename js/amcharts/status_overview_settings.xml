<settings> 
  <data_type>csv</data_type>
  <csv_separator></csv_separator>      
  <skip_rows>1</skip_rows>
  <font></font>      
  <text_size></text_size> 
  <text_color></text_color>
  <decimals_separator></decimals_separator>
  <thousands_separator></thousands_separator> 
  <digits_after_decimal></digits_after_decimal>
  <reload_data_interval></reload_data_interval> 
  <preloader_on_reload></preloader_on_reload>   
  <redraw></redraw>    
  <add_time_stamp></add_time_stamp>
  <precision></precision>                            
  <exclude_invisible></exclude_invisible>            
                                                     
  <pie>
    <x>50</x>
    <y>55</y>
    <radius>50</radius>
    <inner_radius></inner_radius>
    <height>2</height>           
    <angle>0</angle>             
    <outline_color></outline_color>
    <outline_alpha></outline_alpha>
    <base_color></base_color>      
    <brightness_step></brightness_step>
    <colors>0x00cc00,0xcc0000</colors>                  
    <link_target></link_target>        
    <alpha></alpha>                    
  </pie>
  
  <animation>
    <start_time>0</start_time>         
    <start_effect>strong</start_effect>
    <start_radius></start_radius>      
    <start_alpha></start_alpha>        
    <pull_out_on_click></pull_out_on_click>
    <pull_out_time>1.5</pull_out_time>     
    <pull_out_effect>Bounce</pull_out_effect>
    <pull_out_radius></pull_out_radius>      
    <pull_out_only_one></pull_out_only_one>  
  </animation>
  
  <data_labels>
    <radius></radius>                        
    <text_color></text_color>                
    <text_size></text_size>                  
    <max_width></max_width>                  
    <show>      
    </show>
    <show_lines></show_lines>                
    <line_color></line_color>                
    <line_alpha></line_alpha>                
    <hide_labels_percent>0</hide_labels_percent>        
  </data_labels>

  <group>
    <percent></percent>                                 
    <color></color>                                     
    <title></title>                                     
    <url></url>                                         
    <description></description>                         
    <pull_out></pull_out>                               
  </group>

  <background>                                          
    <color></color>                                     
    <alpha></alpha>                                     
    <border_color></border_color>                       
    <border_alpha></border_alpha>                       
    <file></file>                                       
                                                        
  </background>
  
  <balloon>                                             
    <enabled></enabled>                                 
    <color></color>                                     
    <alpha>80</alpha>                                   
    <text_color></text_color>                           
    <text_size></text_size>                             
    <show>
       <![CDATA[{title}: {value}%]]>       
    </show>
  </balloon>
    
  <legend>                                              
    <enabled>false</enabled>                                 
    <x></x>                                             
    <y></y>                                             
    <width></width>                                     
    <color></color>                                     
    <max_columns></max_columns>                         
    <alpha></alpha>                                     
    <border_color></border_color>                       
    <border_alpha>20</border_alpha>                     
    <text_color></text_color>                           
    <text_size></text_size>                             
    <spacing>9</spacing>                                
    <margins>5</margins>                                
    <reverse_order></reverse_order>                     
    <key>                                               
      <size></size>                                     
      <border_color></border_color>                     
    </key>
    <values>                                            
      <enabled></enabled>                               
      <width></width>                                   
      <text><![CDATA[]]></text>                         
     </values>    
  </legend>  
  
  <export_as_image>                                     
    <file></file>                                       
    <target></target>                                   
    <x></x>                                             
    <y></y>                                             
    <color></color>                                     
    <alpha></alpha>                                     
    <text_color></text_color>                           
    <text_size></text_size>                             
  </export_as_image>
  
  <error_messages>                                      
    <enabled>false</enabled>                                 
    <x></x>                                             
    <y></y>                                             
    <color></color>                                     
    <alpha></alpha>                                     
    <text_color></text_color>                           
    <text_size></text_size>                             
  </error_messages>    
  <strings>
    <no_data></no_data>                                 
    <export_as_image></export_as_image>                 
    <collecting_data></collecting_data>                 
  </strings>  
  <labels>                                              
    <label>
      <x>0</x>                                          
      <y>10</y>                                         
      <rotate>false</rotate>                            
      <width></width>                                   
      <align>center</align>                             
      <text_color></text_color>                         
      <text_size>12</text_size>                         
      <text>                                            
      </text>        
    </label>
  </labels>
</settings>
